class Employee:
    def set_data(self,n,a,s):
        self.name = n
        self.age = a
        self.salary = s
    def disp_data(self):
        print(self.name, self.age, self.salary)
e1 = Employee()
e2 = Employee()
e1.set_data("Khushwant",20,25000)
e2.set_data("nitesh",19,000)
e1.disp_data()
e2.disp_data()
